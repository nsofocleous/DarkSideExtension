{
    "name": "Dark Side Extension",
    "description": "Change webpages to a dark theme and back",
    "version": "1.0.0",
    "manifest_version": 2,

    "background": {
        "scripts": ["Content.js"],
        "persistent": true
    },

    "browser_action": {
        "default_title": "Dark Side Extension",
        "default_chrext": "chrext.html"
    },

    "permissions": [
        "contextMenus",
        "https://*/*",
        "http://*/*",
        "tabs"
    ]
}